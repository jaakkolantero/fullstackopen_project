{
  "name": "pizza-italia",
  "version": 2,
  "github": {
    "enabled": true
  },
  "builds": [
    {
      "src": "www/next.config.js",
      "use": "@now/next"
    },
    { "src": "api/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "www/$1",
      "continue": true
    }
  ]
}
